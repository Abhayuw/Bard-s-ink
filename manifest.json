{
    "manifest_version": 3,
    "name": "Bard's Ink",
    "version": "1.0",
    "description": "Turns HidustanTimes website's headlines into mini rhymes",
    "permissions": ["activeTab"], 
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "ink pen1.png",
        "48": "ink pen1.png", 
        "128": "ink pen1.png" 
      }
  },
    "content_scripts": [
      {
        "matches": ["https://www.hindustantimes.com/"],
        "js": ["content.js"]
        
      }
    ]
  }
  